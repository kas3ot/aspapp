@model app.Models.Video
@{
    ViewBag.Title = "image";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>image</h2>

@using (Html.BeginForm("image", "Videos", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <input type="file" name="image" />
    <input type="submit" value="Upload" />
}